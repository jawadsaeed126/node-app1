{
    "containerDefinitions": [
      {
        "name": "demo-node-app-1-task-definition-container-api1",
        "image": "private-ecr-repository-app1-node-api1:latest",
        "cpu": 100,
        "memory": 128,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000
          }
        ],
        "environment": [
          {
            "name": "NODE_ENV",
            "value": "production"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/demo-node-app-1-task-definition-container-api1",
            "awslogs-region": "eu-west-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "family": "demo-node-app-1-task-definition-api1"
  }
  
  